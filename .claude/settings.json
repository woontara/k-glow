{
  "permissions": {
    "allow": [
      "Bash(npm install:*)",
      "Bash(npx prisma generate:*)",
      "Bash(tree:*)",
      "Bash(npm run build:*)",
      "Bash(npx prisma db push:*)",
      "Bash(ls:*)",
      "Bash(npx tsx:*)",
      "Bash(npm run dev:*)",
      "Bash(curl:*)",
      "Bash(python:*)",
      "Bash(npm run db:migrate:*)",
      "Bash(npm run db:push:*)",
      "Bash(npm run db:generate:*)",
      "Bash(tee:*)",
      "Bash(git status:*)",
      "Bash(git diff:*)",
      "Bash(git log:*)"
    ],
    "ask": [
      "Bash(git push:*)",
      "Bash(git commit:*)",
      "Bash(rm:*)",
      "Bash(del:*)",
      "WebFetch"
    ],
    "deny": [
      "Read(./.env)",
      "Read(./.env.*)",
      "Read(./secrets/**)",
      "Read(./.git/config)",
      "Edit(./.env)",
      "Edit(./.env.*)",
      "Edit(./secrets/**)"
    ]
  }
}
